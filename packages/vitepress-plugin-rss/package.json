{
  "name": "vitepress-plugin-rss",
  "version": "0.2.6",
  "description": "vitepress plugin, generate feed.rss",
  "author": "sugar",
  "license": "MIT",
  "homepage": "https://github.com/cnjimbo/cnjimbo.github.io/tree/master/packages/vitepress-plugin-rss",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnjimbo/cnjimbo.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/cnjimbo/cnjimbo.github.io/issues"
  },
  "keywords": [
    "vitepress",
    "plugin",
    "rss",
    "feed"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npx tsup src/index.ts --dts --watch --format esm,cjs --external vitepress",
    "build": "npx tsup src/index.ts --dts --format esm,cjs --external vitepress --silent"
  },
  "peerDependencies": {
    "vitepress": "^1"
  },
  "dependencies": {
    "cross-spawn": "^7.0.3",
    "fast-glob": "^3.3.2",
    "feed": "^4.2.2",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/cross-spawn": "^6.0.6",
    "vite": "^5.2.13",
    "vitepress": "1.2.3"
  }
}

{
  "name": "@sugarat/theme",
  "version": "0.2.16",
  "description": "简约风的 Vitepress 博客主题，sugarat vitepress blog theme",
  "author": "sugar",
  "license": "MIT",
  "homepage": "https://theme.sugarat.top",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ATQQ/sugar-blog.git"
  },
  "bugs": {
    "url": "https://github.com/ATQQ/sugar-blog/issues"
  },
  "keywords": [
    "vitepress",
    "theme",
    "粥里有勺糖"
  ],
  "exports": {
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.mjs",
      "require": "./dist/node.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "node.d.ts",
    "node.js",
    "src",
    "types"
  ],
  "scripts": {
    "dev": "pnpm run build:node && pnpm run dev:docs",
    "dev:docs": "vitepress dev docs",
    "dev:node": "pnpm tsup",
    "build": "pnpm run build:node && pnpm run build:docs",
    "build:docs": "pnpm vitepress build docs",
    "build:node": "pnpm tsup",
    "serve": "vitepress serve docs",
    "documate:upload": "documate upload"
  },
  "dependencies": {
    "@giscus/vue": "^3.0.0",
    "@mdit-vue/shared": "^2.1.3",
    "@mermaid-js/mermaid-mindmap": "^9.3.0",
    "@vue/shared": "^3.4.27",
    "@vueuse/core": "^10.9.0",
    "fast-glob": "^3.3.2",
    "gray-matter": "^4.0.3",
    "markdown-it-task-checkbox": "^1.0.6",
    "mermaid": "^10.9.1",
    "vitepress-plugin-mermaid": "2.0.16",
    "vitepress-plugin-pagefind": "^0.3.2",
    "vitepress-plugin-rss": "^0.2.4",
    "vitepress-plugin-tabs": "0.5.0"
  },
  "devDependencies": {
    "@documate/documate": "^0.2.1",
    "@documate/vue": "^0.3.1",
    "@element-plus/icons-vue": "^2.3.1",
    "element-plus": "^2.7.3",
    "esbuild-plugin-vue-next": "^0.1.4",
    "esbuild-sass-plugin": "^3.3.0",
    "eslint-plugin-vue": "^9.26.0",
    "javascript-stringify": "^2.1.0",
    "pagefind": "1.1.0",
    "sass": "^1.77.2",
    "typescript": "^5.4.5",
    "vitepress": "1.2.2",
    "vue": "^3.4.27"
  }
}

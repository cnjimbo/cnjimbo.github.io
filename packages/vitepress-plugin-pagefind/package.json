  "license": "MIT",
  "homepage": "https://github.com/cnjimbo/cnjimbo.github.io/tree/master/packages/vitepress-plugin-pagefind",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnjimbo/cnjimbo.github.io.git"
  },
  "bugs": {
    "url": "https://github.com/cnjimbo/cnjimbo.github.io/issues"
